    "build": "npm run lib && vue-cli-service build --dest dist src/main.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kakajun/kaka-excel-js.git"
  },
  "bugs": {
    "url": "https://github.com/kakajun/kaka-excel-js/issues"
  },
  "dependencies": {
    "debug": "^4.3.3",
    "core-js": "^3.6.4",
    "file-saver": "^2.0.2",
