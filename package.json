{
  "name": "kaka-excel-js",
  "version": "0.0.7",
  "description": "A tool for exporting excel from the front;vue2",
  "private": false,
  "main": "lib/kakaExcelJs.umd.js",
  "unpkg": "lib/kakaExcelJs.umd.min.js",
  "keywords": [
    "excel",
    "table",
    "xlsx",
    "vxe-table"
  ],
  "files": [
    "lib"
  ],
  "license": "MIT",
  "author": "pikaz",
  "scripts": {
    "dev": "vue-cli-service serve src/main.js",
