    "vue": "^2.6.14",
    "xlsx": "^0.17.4"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "element-ui": "^2.15.6",
    "gh-pages": "^3.2.3",
    "kaka-excel-js": "^0.0.1",
    "vue-template-compiler": "^2.6.11",
    "vxe-table": "^3.4.12",
    "xe-utils": "^3.5.1"
  },
  "browserslist": [
